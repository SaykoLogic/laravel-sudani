    "homepage": "https://github.com/alkhatibdev/laravel-sudani",
    "keywords": [
        "sudani",
        "spay",
        "sudan",
        "integration",
        "laravel"
    ],
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "AlkhatibHamad",
            "email": "me@alkhatib.dev",
            "homepage": "https://github.com/alkhatibdev",
